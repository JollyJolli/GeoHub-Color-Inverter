{
  "manifest_version": 3,
  "name": "GeoHub Color Inverter",
  "version": "1.1",
  "description": "Invierte los colores durante las partidas de GeoHub",
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "icons": {
    "48": "icon48.svg",
    "128": "icon128.svg"
  }
}